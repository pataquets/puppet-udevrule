# This udev rule is managed by Puppet. Changes will be lost!
<%- if kernel then -%>KERNEL=="<%= kernel-%>",<%- end -%>
<%- if driver then -%>DRIVER=="<%= driver-%>",<%- end -%>
<%- if devpath then -%>DEVPATH=="<%= devpath-%>",<%- end -%>
<%- if action then -%>ACTION=="<%=action-%>",<%- end -%>
<%- if subsystem then -%>SUBSYSTEM=="<%=subsystem-%>",<%- end -%>
<%- if subsystems then -%>SUBSYSTEMS=="<%=subsystems-%>",<%- end -%>
<%- if name_match then -%>NAME=="<%= name_match-%>",<%- end -%>
<%- if symlink_match then -%>SYMLINK=="<%=symlink_match-%>",<%- end -%>
<%- if name_set then -%>NAME="<%= name_set-%>",<%- end -%>
<%- if symlink_add then -%>SYMLINK+="<%=symlink_add-%>",<%- end -%>
<%- if symlink_set then -%>SYMLINK="<%=symlink_set-%>",<%- end -%>
<%- if attrs then -%>
<%- attrs.each do |attr| -%>
<%- attr.each do |name, value| %>ATTRS{<%=name-%>}=="<%=value-%>",<%- end -%>
<%- end -%>
<%- end -%>
<%- if run_cmd then -%>RUN+="<%=run_cmd-%>",<%- end -%>
<%- if owner then -%>OWNER="<%=owner-%>",<%- end -%>
<%- if group then -%>GROUP="<%=group-%>",<%- end -%>
<%- if mode then -%>MODE="<%=mode-%>",<%- end -%>
<%- if options then -%>OPTIONS+="<%=options-%>",<%- end -%>
